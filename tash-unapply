[32m+[m
[32m+[m[32m        #Création du Formulaire[m
[32m+[m[32m        $form = $this->createForm(ArticleFormType::class, $article)[m
[32m+[m[32m            ->handleRequest($request);[m
[32m+[m
[32m+[m[32m        #Si le formulaire est soumis et valide[m
[32m+[m[32m        if ($form->isSubmitted() && $form->isValid()){[m
[32m+[m[32m            #dump($article);[m
[32m+[m[32m            #1.Traitement de l'upload de l'image[m
[32m+[m
[32m+[m[32m            /** @var UploadedFile $featuredImage */[m
[32m+[m[32m            $featuredImage = $article->getFeaturedImage();[m
[32m+[m
[32m+[m[32m            $fileName = $this->slugify($article->getTitre()) . '.' . $featuredImage->guessExtension();[m
[32m+[m
[32m+[m[32m            // Move the file to the directory where brochures are stored[m
[32m+[m[32m            try {[m
[32m+[m[32m                $featuredImage->move([m
[32m+[m[32m                    $this->getParameter('articles_assets_dir'),[m
[32m+[m[32m                    $fileName[m
[32m+[m[32m                );[m
[32m+[m[32m            } catch (FileException $e) {[m
[32m+[m[32m                // ... handle exception if something happens during file upload[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            #Mise à jour de l'image[m
[32m+[m[32m            $article->setFeaturedImage($fileName);[m
[32m+[m
[32m+[m[32m            #Mise à jour du slug[m
[32m+[m[32m            $article->setSlug($this->slugify($article->getTitre()));[m
[32m+[m
[32m+[m[32m            #Sauvegarde en BDD[m
[32m+[m[32m            $em = $this->getDoctrine()->getManager();[m
[32m+[m[32m            $em->persist($article);[m
[32m+[m[32m            $em->flush();[m
[32m+[m
[32m+[m[32m            #Notification[m
[32m+[m[32m            $this->addFlash('notice',[m
[32m+[m[32m                'Félicitations, votre article est en ligne!');[m
[32m+[m
[32m+[m[32m            #Redirection[m
[32m+[m[32m            return $this->redirectToRoute('category', [[m
[32m+[m[32m                'slug' => $article->getSlug(),[m
[32m+[m[32m                'id' =>$article->getId() ]);[m
[32m+[m[32m            // ... persist the $product variable or any other work[m
[32m+[m
[32m+[m[32m        }[m
