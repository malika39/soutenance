[32m+[m[32m    /**[m
[32m+[m[32m     * Démonstration de l'ajout d'un Article avec Doctrine ![m
[32m+[m[32m     * @Route("/blog/test", name="article_demo")[m
[32m+[m[32m     */[m
[32m+[m[32m    public function demo()[m
[32m+[m[32m    {[m
[32m+[m[32m        #Appel du membre[m
[32m+[m[32m        $user = $this->getDoctrine()[m
[32m+[m[32m            ->getRepository(User::class)[m
[32m+[m[32m            ->find(1);[m
[32m+[m
[32m+[m[32m        #Création de l'Article[m
[32m+[m[32m        $article = new Article();[m
[32m+[m[32m        $article[m
[32m+[m[32m            ->setTitre("Galette des rois : la meilleure recette")[m
[32m+[m[32m            ->setSlug("galette-des-rois-la-meilleure-recette")[m
[32m+[m[32m            ->setContenu("<p>Pour faire une super galette, il vous faut de la pâte feuilleté, du beurre et de la poudre d'amandes !</p>")[m
[32m+[m[32m            ->setFeaturedImage("croissant.jpg")[m
[32m+[m[32m            ->setDateCreation(\DateTime::createFromFormat('Y-m-d', "2018-01-16"))[m
[32m+[m[32m            ->setUser($user);[m
[32m+[m
[32m+[m[32m        /*[m
[32m+[m[32m         * Récupération du Manager de Doctrine.[m
[32m+[m[32m         * Le Entity Manager est une classe qui sait[m
[32m+[m[32m         * comment persister d'autres classes.[m
[32m+[m[32m         * (Effectuer des opérations CRUD sur nos entités).[m
[32m+[m[32m         */[m
[32m+[m
[32m+[m[32m        $em = $this->getDoctrine()->getManager();[m
[32m+[m[32m        $em->persist($user);[m
[32m+[m[32m        $em->persist($article);[m
[32m+[m[32m        $em->flush();[m
[32m+[m
[32m+[m[32m        return new Response('Nouvel article ajouté avec ID:' . $article->getId() . 'de Auteur:' . $user->getLastName());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Formulaire pour ajouter un article.[m
[32m+[m[32m     * @Route ("/blog/creer-un-article", name="article_new")[m
[32m+[m[32m     * @Security("has_role('ROLE_ADMIN')")[m
[32m+[m[32m     */[m
[32m+[m
[32m+[m[32m    public function newArticle(Request $request)[m
[32m+[m[32m    {[m
