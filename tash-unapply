[32m+[m[32m    public function buildForm(FormBuilderInterface $builder, array $options)[m
[32m+[m[32m    {[m
[32m+[m[32m        $builder->add('titre',TextType::class, [[m
[32m+[m[32m            'required'=>true,[m
[32m+[m[32m            'label'=>"Titre",[m
[32m+[m[32m            'attr' => [[m
