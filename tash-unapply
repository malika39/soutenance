[32m+[m[32m            ][m
[32m+[m[32m        ])[m
[32m+[m
[32m+[m[32m            ->add('contenu', TextareaType::class, [[m
[32m+[m[32m                'label' => "Contenu"[m
[32m+[m[32m            ])[m
[32m+[m
[32m+[m[32m            ->add('featuredImage', FileType::class, [[m
[32m+[m[32m                'label' => "Image",[m
[32m+[m[32m                'attr' => [[m
[32m+[m[32m                    'class' =>'dropify'[m
[32m+[m[32m                ][m
[32m+[m[32m            ])[m
[32m+[m
[32m+[m[32m            ->add('submit', SubmitType::class, [[m
[32m+[m[32m                'label' => 'Publier mon article'[m
[32m+[m[32m            ]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function configureOptions(OptionsResolver $resolver)[m
[32m+[m[32m    {[m
[32m+[m[32m        $resolver->setDefaults([[m
[32m+[m[32m            'date_class' => Article::class[m
[32m+[m[32m        ]);[m
[32m+[m[32m    }[m
